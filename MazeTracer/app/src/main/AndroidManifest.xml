<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeffrey.mazetracer" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <catagory android:name="android.intent.catagory.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InstructionActivity"
            android:label="@string/title_activity_instruction" >
        </activity>
        <activity
            android:name=".LevelTemplates"
            android:label="@string/title_activity_level_templates" >
        </activity>
        <activity
            android:name=".WaitingActivity"
            android:label="@string/title_activity_waiting" >
        </activity>
    </application>

</manifest>
