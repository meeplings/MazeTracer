<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.jeffrey.mazetracer.MainActivity"
    android:id = "@+id/tableLayout">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:id="@+id/star"
            android:layout_row="2"
            android:layout_column="1"
            android:background="@android:drawable/star_on" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX00"
            android:background="@drawable/red_x"
            android:layout_below="@+id/star"
            android:layout_column="7" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX01"
            android:background="@drawable/red_x"
            android:layout_column="8" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX02"
            android:background="@drawable/red_x"
            android:layout_column="9" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX03"
            android:background="@drawable/red_x"
            android:layout_column="10" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX10"
            android:background="@drawable/red_x"
            android:layout_column="7" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX11"
            android:background="@drawable/red_x"
            android:layout_column="8" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX12"
            android:background="@drawable/red_x"
            android:layout_column="9" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX13"
            android:background="@drawable/red_x"
            android:layout_column="10" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX20"
            android:background="@drawable/red_x"
            android:layout_column="7" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX21"
            android:background="@drawable/red_x"
            android:layout_column="8" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX22"
            android:background="@drawable/red_x"
            android:layout_column="9" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX23"
            android:background="@drawable/red_x"
            android:layout_column="10" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX30"
            android:background="@drawable/red_x"
            android:layout_column="7" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX31"
            android:background="@drawable/red_x"
            android:layout_column="8" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX32"
            android:background="@drawable/red_x"
            android:layout_column="9" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/redX33"
            android:background="@drawable/red_x"
            android:layout_below="@+id/redX00"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_column="10" />
    </TableRow>


</TableLayout>



